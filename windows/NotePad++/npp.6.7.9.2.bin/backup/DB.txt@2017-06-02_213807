select tid,name, rtrim(ltrim(substring(email,1,patindex('%,%',email)-1))) as pass
,rtrim(ltrim(substring(email,patindex('%,%',email)+1,len(email)-patindex('%,%',email)))) as email  from ty
  
create function split(@str varchar(100))
returns   @tab table(id int identity(1,1),val varchar(10))
as begin
declare @val varchar(10),@ind int 
set @ind=charindex(',',@str)
while @ind>0
begin
set @val=left(@str,@ind-1)
set @str=right(@str,len(@str)-@ind)
set @ind=charindex(',',@str)
insert into @tab
values(@val)
end
if @ind=0
set @val=@str
insert into @tab
values(@val)
return  
end


